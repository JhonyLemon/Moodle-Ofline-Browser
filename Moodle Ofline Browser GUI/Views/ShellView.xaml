        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Moodle_Ofline_Browser_GUI.Views"
        mc:Ignorable="d"
        Title="ShellView" Height="650" Width="800">
    <Grid>
        <Menu VerticalAlignment="Top" Height="20">
            <MenuItem Header="Plik">
                <MenuItem Header="Otwórz"/>
                <MenuItem Header="Zamknij"/>
            </MenuItem>
        </Menu>
        <ContentControl x:Name="ActiveItem" Margin="0,20,0,0"/>
